{
  "name": "PWchat",
  "short_name": "PWchat",
  "description": "chatfiles",
  "lang": "en-US",
  "start_url": "./",
  "scope": "./",
  "theme_color": "#181c25",
  "background_color": "#181c25",
  "display": "standalone",
  "display_override": [
    "window-controls-overlay"
  ],
  "edge_side_panel": {
    "preferred_width": 496
  },
  "icons": [
    {
      "src": "./favicon-48.png",
      "sizes": "48x48"
    },
    {
      "src": "./favicon-96.png",
      "sizes": "96x96"
    },
    {
      "src": "./favicon-128.png",
      "sizes": "128x128"
    },
    {
      "src": "./favicon-256.png",
      "sizes": "256x256"
    },
    {
      "src": "./favicon-512.png",
      "sizes": "512x512"
    }
  ], 
  
  "protocol_handlers": [
    {
      "protocol": "web+chat",
      "url": "./?cmd=%s"
    }
  ] 
 
}